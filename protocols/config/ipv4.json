  "header": {
    "version": {
      "offset": 0,
      "length": 4,
      "description": "IPv4-Version"
    },
    "header_length": {
      "offset": 4,
      "length": 4,
      "description": "Header Length"
    },
    "tos": {
      "offset": 8,
      "length": 8,
      "description": "Type of Service"
    },
    "total_length": {
      "offset": 16,
      "length": 16,
      "description": "Total Length"
    },
    "identification": {
      "offset": 32,
      "length": 16,
      "description": "Identification"
    },
    "flags": {
